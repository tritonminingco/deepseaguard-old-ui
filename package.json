{
  "name": "deepseaguard-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "server:install": "cd server && npm install",
    "demo": "chmod +x demo-fathomnet.sh && ./demo-fathomnet.sh",
    "start": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "build:prod": "npm run lint && npm run build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "cd src/tests && node run_tests.js",
    "test:integration": "cd server && npm test",
    "clean": "rm -rf dist server/dist node_modules server/node_modules",
    "setup": "npm install && npm run server:install",
    "health": "curl -f http://localhost:3000/api/health || echo 'Server not running'",
    "docs": "echo 'Opening documentation...' && open docs/user_guide.md"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@react-three/drei": "^9.77.0",
    "@react-three/fiber": "^8.13.0",
    "axios": "^1.11.0",
    "chart.js": "^4.3.0",
    "esri-leaflet": "^3.0.17",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "pg": "^8.16.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "recharts": "^2.5.0",
    "socket.io-client": "^4.8.1",
    "three": "^0.153.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.3.9"
  }
}
